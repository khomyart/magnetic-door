void draw(void)
{
  /* Draw screen title */
  u8g.setFont(u8g_font_9x15B);
  u8g.setPrintPos(128/2 - u8g.getStrWidth(screenName[currentWindowNumber])/2,55);
  u8g.print(screenName[currentWindowNumber]);
  /* Draw image */
  u8g.drawBitmapP(48, 3, 4, 32, framebuffer[currentWindowNumber][currentFrame]);
}

void updateScreen()
{
  if (currentFrame == 2)
  {
    currentFrame = 0;
  } else 
  {
    currentFrame++;
  }
}

void clearDisplay() {
  u8g.firstPage();
  do {
  } while ( u8g.nextPage() );
};

void setup() {
  clearDisplay();
  Serial.begin(9600);
}

void loop() {
  if (time + frameTime < millis())
  {
    u8g.firstPage();
    do {
      draw();
    } while ( u8g.nextPage() );
    updateScreen();
    time = millis();
  }
}
